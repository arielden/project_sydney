{
  "name": "project_sidney",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "echo '⚠️  Build skipped in development mode. Use npm run build:prod for production builds.'",
    "build:prod": "tsc -b && vite build",
    "preview": "echo '⚠️  Preview skipped in development mode. Use npm run preview:prod for production preview.'",
    "preview:prod": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "simulate:attempts": "node scripts/simulate-attempts.cjs",
    "simulate:elo": "node scripts/simulate-elo-evolution.cjs",
    "simulate:elo:smoothed": "node scripts/simulate-elo-evolution.cjs 1000 500 true",
    "simulate:elo:staged": "node scripts/simulate-elo-evolution.cjs 1000 500 false"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.1",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
